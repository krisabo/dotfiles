# alias definitions

alias tmux="tmux -2"
alias li='du -cksh * | sort -h'
alias gis="git status"
alias gka="gitk --all"
alias j="z" # jump for FASD

alias cd..="cd .."
alias ...="cd ../../"
alias ....="cd ../../../"
alias .....="cd ../../../../"
alias ......="cd ../../../../../"
alias .2="cd ../.."
alias .3="cd ../../.."
alias .4="cd ../../../.."
alias .5="cd ../../../../.."
